@model List<tpn7.EF.MVC.Models.SuppliersView>

@{
    ViewBag.Title = "Suppliers Page";
}
<h1>@ViewBag.Title</h1>


<table class="table">
        <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Descripcion</th>
                <th scope="col">Contact</th>
                <th scope="col">Contact Title</th>
                <th scope="col">Adress</th>
                <th scope="col">City</th>
                <th scope="col">Action</th>
            </tr>
        </thead>
        <tbody>
        </tbody>

            @foreach (var supplier in Model)
            {
                <tr>
                    <th scope="row">@supplier.Id</th>
                    <td>@supplier.Description</td>
                    <td>@supplier.Contacto</td>
                    <td>@supplier.ContactoTitle</td>
                    <td>@supplier.Direccion</td>
                    <td>@supplier.Ciudad</td>
                    <td>@Html.ActionLink("Modificar", "Update", new {id = supplier.Id})</td>
                    <td>@Html.ActionLink("Borrar", "Delete", new {id = supplier.Id}, new {onclick = "return confirm ('¿Desea borrar este registro?')"})</td>
                </tr>
            }
 </table>

<div>
    @Html.ActionLink("Insertar Nuevo Supplier", "Insert")
</div>
