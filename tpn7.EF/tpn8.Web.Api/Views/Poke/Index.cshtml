@model tpn8.Web.Api.Models.Pokemon


@{
    ViewBag.Title = "Pokemon page";
}

<h1>@ViewBag.Title</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">URL</th>
        </tr>
    </thead>
    <tbody>
    </tbody>

    @foreach (var pokemon in Model.Forms)
    {
    <tr>
        <th scope="row">@pokemon.Name</th>
        <th scope="row">@pokemon.Url</th>
        

    </tr>
    }




